<table class="table", width="100%" style="table-layout: fixed;">
	<% current_end_user.followings.each do |following| %>
		<% following.posts.each do |post| %>

			<tr>
				<td width="40%">
					<%= post.tweet %>
				</td>
				<td width="30%">
					<%= post.end_user.nickname %>
				</td>
				<td width="30%">
					<%= post.created_at %>
				</td>
			</tr>

		<% end %>
	<% end %>
</table>
